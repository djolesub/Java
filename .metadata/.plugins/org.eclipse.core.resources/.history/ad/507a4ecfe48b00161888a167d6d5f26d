package intro;
import java.net.*;

public class NetworksIntro {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			InetAddress ip = InetAddress.getByName("www.google.com");
			System.out.println(ip);
		}
		catch(UnknownHostException e){
			e.printStackTrace();
		}
		
		
		
	}

}
