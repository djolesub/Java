package circleCylindar;

public class Circle {
	private double radius;
	private String color;
	
	public Circle(){
		this.radius = 1.0;
		this.color = "red";
	} 
	
	public Circle(double radius,String color){
		this.radius = radius;
		this.color = color;
	} 
	
	public Circle(double radius){
		this.radius = radius;
		this.color="red";
	}
	
	
	
	
	
	
	
}
