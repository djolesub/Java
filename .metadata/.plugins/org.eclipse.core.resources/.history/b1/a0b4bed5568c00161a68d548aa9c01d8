package intro;
import java.net.*;

public class NetworksIntro {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		InetAddress ip ;
		try {
			 ip = InetAddress.getByName("www.machine.com");
			 System.out.println(ip);
		
			
			
		}
		catch(UnknownHostException e){
			e.printStackTrace();
		}
		
		
		
	}

}
