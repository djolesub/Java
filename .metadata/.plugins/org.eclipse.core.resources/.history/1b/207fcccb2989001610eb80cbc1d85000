package enumType;

enum Car { Mercedes,BMW,Audi,Jugo,Skoda};

public class EnumDemo {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car c;
		c = Car.Mercedes;
		System.out.println("Car Information are: "+c); 
		
		if(c == Car.Mercedes){
			System.out.println("They are Equal");
		}
		
		
		
		
	}

}
